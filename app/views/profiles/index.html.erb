<p id="notice"><%= notice %></p>

<h1>Profiles</h1>

<table class= 'table'>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Birth</th>
      <th>Gender</th>
      <th>Phone</th>
      <th>Address</th>
      <th>City</th>
      <th>State</th>
      <th>Zip code</th>
      <th>License</th>
      <th>Driver license</th>
      <th>User</th>
      <th>Speciality</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profiles.each do |profile| %>
      <tr>
        <td><%= profile.first_name %></td>
        <td><%= profile.last_name %></td>
        <td><%= profile.birth %></td>
        <td><%= profile.gender %></td>
        <td><%= profile.phone %></td>
        <td><%= profile.address %></td>
        <td><%= profile.city %></td>
        <td><%= profile.state %></td>
        <td><%= profile.zip_code %></td>
        <td><%= profile.license %></td>
        <td><%= profile.driver_license %></td>
        <td><%= profile.user_id %></td>
        <td><%= profile.speciality_id %></td>
        <td><%= link_to 'Show', profile %></td>
        <td><%= link_to 'Edit', edit_profile_path(profile) %></td>
        <td><%= link_to 'Destroy', profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile', new_profile_path %>
